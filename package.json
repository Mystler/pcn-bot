{
  "name": "prismatic-carrier-network",
  "type": "module",
  "version": "1.1.0",
  "description": "Discord Bot to announce Fleet Carrier updates via EDDN data",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node --enable-source-maps .",
    "register": "node --enable-source-maps dist/register.js",
    "build": "tsc",
    "clean": "rm -r dist",
    "lint": "prettier --check src && eslint src",
    "format": "prettier --write src"
  },
  "author": "Mystler",
  "dependencies": {
    "discord.js": "^14.19.3",
    "handlebars": "^4.7.8",
    "zeromq": "^6.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/node": "^22.15.19",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "zeromq"
    ]
  }
}
